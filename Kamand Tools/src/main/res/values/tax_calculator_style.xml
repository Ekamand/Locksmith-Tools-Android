<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Date_Text_Style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:text">"@string/Date_Text"</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#164A53</item>
        <item name="android:textSize">15dp</item>
        <item name="android:paddingRight">5dp</item>

    </style>

    <style name="EditText_Style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">.65</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">25dp</item>
        <item name="android:maxLength">9</item>
        <item name="android:gravity">center</item>
        <item name="android:selectAllOnFocus">false</item>

    </style>


</resources>